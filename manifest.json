{
    "name": "Turn on Speech Input",
    "description": "Turns on the speech attribute, allows you to speak into an input",
    "version": "0.2",
    "manifest_version": 2,
    "permissions": ["contextMenus"],
    "minimum_chrome_version": "6",
    "background": {
        "scripts": [ "background.js" ]
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["input-speech.js"]
    }]
}
